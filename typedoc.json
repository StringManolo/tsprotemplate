{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/tests/**"
  ],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "includeVersion": true,
  "readme": "README.md",
  "name": "Bahamut Scanner",
  "navigationLinks": {
    "GitHub": "https://github.com/StringManolo/Bahamut"
  },
  "categorizeByGroup": true,
  "categoryOrder": [
    "CLI",
    "Core",
    "Modules",
    "Utils",
    "*"
  ],
  "sort": ["source-order"],
  "plugin": []
}
